{
    "name": "dhope0000/infrastructure",
    "require": {
        "php": "^8.4",
        "php-http/guzzle7-adapter": "^1.1",
        "php-di/php-di": "7.1.1",
        "dhope0000/lxd": "^1.2.0",
        "vlucas/phpdotenv": "^5.6",
        "symfony/http-foundation": "^7.3.6",
        "symfony/filesystem": "^7.3.6",
        "symfony/cache": "^7.3.6",
        "crunzphp/crunz": "^3.8.0",
        "symfony/routing": "^7.3.6",
        "robmorgan/phinx": "^0.16.10"
    },
    "autoload": {
        "psr-4": {
            "dhope0000\\LXDClient\\": "src/classes"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4.2",
        "phpstan/phpstan": "^2.1.31"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
